Private Sub Form_Load()
 Set con = New ADODB.Connection
 Set rs = New ADODB.Recordset
 rs.LockType = adLockOptimistic
 con.ConnectionString = "Provider=Microsoft.Jet.OLEDB.4.0;Data Source=C:\Documents and Settings\Administrator\Desktop\bms\Database\movdetail.mdb;Persist Security Info=False"
 
con.Open
rs.Open "Select * from movdetails WHERE ID = " & MOVIE_ID & ";", con

If rs.EOF = False Then
Option1.Caption = rs.Fields(6)
Option2.Caption = rs.Fields(7)
Option3.Caption = rs.Fields(8)
Option4.Caption = rs.Fields(9)
Text1.Text = rs.Fields(10)
Label3.Caption = rs.Fields(1)
End If

rs.Close
con.Close


con.Open
rs.Open "Select * from show_timing where MOVIE_ID=" & MOVIE_ID & ";", con

If rs.EOF = False Then

Do
  seat_no = rs.Fields(3)
  For i = 0 To 96
  
     If i = seat_no Then
        Check1(i).Enabled = False
     End If
  
  Next i
   rs.MoveNext
   
Loop While rs.EOF = False


End If

