Private Sub Check1_Click(Index As Integer)

  If Check1(Index).Value = 1 Then
    seats(Index) = True
  Else
    seats(Index) = False
  End If

End Sub

Private Sub getTime()

 If Option1.Value = True Then
   time = Val(Option1.Caption)
 ElseIf Option2.Value = True Then
    time = Val(Option2.Caption)
 ElseIf Option3.Value = True Then
   time = Val(Option3.Caption)
 ElseIf Option4.Value = True Then
    time = Val(Option4.Caption)
 End If

End Sub

Private Sub Command1_Click()
getTime
con.Open
rs.Open "show_timing", con

For i = 0 To 96
    If seats(i) = True Then
        rs.AddNew
        rs.Fields(0) = MOVIE_ID
        rs.Fields(1) = Label3.Caption
        rs.Fields(2) = time
        rs.Fields(3) = i
        rs.Update
    End If
Next i
rs.Update
rs.Close

Unload Me
TICKET.Show

End Sub

